{
    "presets": [
        [
            "es2015",
            {
                "modules": false
            }
        ]
    ],
    "plugins": [
        "external-helpers",
        "transform-class-properties",
        [
            "transform-es2015-classes",
            {
                "loose": true
            }
        ],
        "transform-object-assign",
        [
            "transform-react-jsx",
            {
                "pragma": "h"
            }
        ]
    ]
}
